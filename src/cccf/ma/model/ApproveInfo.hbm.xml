<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping SYSTEM "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="cccf.ma.model.ApproveInfo" table="tb_approveinfo">

		<id name="id" type="string" unsaved-value="null" length="36" >
			<column name="sid" not-null="true" />
			<generator class="uuid.hex" />
		</id>
           
		<property name="approverID"  length="40" />
		<property name="approverName"  length="40" />
		<property name="approveResult" length="40" /> 
		<property name="approveMemo" length="4000" />
		<property name="approveDate" /> 
		 
	</class>
</hibernate-mapping>