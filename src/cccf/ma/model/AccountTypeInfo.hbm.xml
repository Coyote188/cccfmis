<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping SYSTEM "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
  <class name="cccf.ma.model.AccountTypeInfo" table="accounttype">  
  
    <id name="atypeId" type="string" unsaved-value="null" >
        <column name="atypeid"  not-null="true"/>
        <generator class="uuid.hex"/>
    </id>
    
    <property name="name" type="string">
         <column name="name"  not-null="false"/>
     </property>
    <property name="desc" type="string">
         <column name="descr"  not-null="false"/>
     </property>
    <property name="isCustom" type="boolean">
         <column name="iscustom"  not-null="false"/>
     </property>
  </class>   
</hibernate-mapping>