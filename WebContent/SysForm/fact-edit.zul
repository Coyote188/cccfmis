						<hbox align="center">
							<label value="*" style="color:#ff0000" />
							<label value="生产企业地址(英文):" />
						</hbox>
						<textbox
							value="@{frmfactureEdit$composer.manu.manuAddressEn }"
							width="200px" />

					</row>
					<row>
						<hbox align="center">
							<label value="*" style="color:#ff0000" />
							<label value="生产企业邮编:" />
						</hbox>
						<textbox
							value="@{frmfactureEdit$composer.manu.manuZip }" width="200px" />

					</row>
					<row>
						<hbox align="center">
							<label value="*" style="color:#ff0000" />
							<label value="生产企业电子邮件:" />
						</hbox>
						<textbox
							value="@{frmfactureEdit$composer.manu.manuEmail }" width="200px" />

					</row>
					<row>
						<hbox align="center">
							<label value="*" style="color:#ff0000" />
							<label value="生产企业联系人:" />
						</hbox>
						<textbox
							value="@{frmfactureEdit$composer.manu.manuLinkMan }"
							width="200px" />

					</row>
					<row>
						<hbox align="center">
							<label value="*" style="color:#ff0000" />
							<label value="生产企业联系电话:" />
						</hbox>
						<textbox
							value="@{frmfactureEdit$composer.manu.manuTel }" width="200px" />

					</row>
					<row>
						<hbox align="center">
							<label value="*" style="color:#ff0000" />
							<label value="生产企业传真:" />
						</hbox>
						<textbox
							value="@{frmfactureEdit$composer.manu.manuFax }" width="200px" />

					</row>
				</rows>
			</grid>
		</groupbox>
		<button id="btnSubmit" width="68px" height="26px" label="提交" />
		<button id="btnClean" width="68px" height="26px" label="清空" />
