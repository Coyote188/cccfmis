							value="@{frmfactureEdit$composer.manu.manuLegalPerson }"
							width="200px" />

					</row>
					<row>
						<hbox align="center">
							<label value="*" style="color:#ff0000" />
							<label value="生产企业机构类型:" />
						</hbox>
						<textbox
							value="@{frmfactureEdit$composer.manu.manuType }" width="200px" />

					</row>
					<row>
						<hbox align="center">
							<label value="*" style="color:#ff0000" />
							<label value="生产企业经济行业:" />
						</hbox>
						<textbox
							value="@{frmfactureEdit$composer.manu.manuEcocalling }"
							width="200px" />

					</row>
					<row>
						<hbox align="center">
							<label value="*" style="color:#ff0000" />
							<label value="生产企业经济行业(新国标代码):" />
						</hbox>
						<textbox
							value="@{frmfactureEdit$composer.manu.manuEcocallingNew }"
							width="200px" />

					</row>
					<row>
						<hbox align="center">
							<label value="*" style="color:#ff0000" />
							<label value="生产企业经济类型:" />
						</hbox>
						<textbox
							value="@{frmfactureEdit$composer.manu.manuEcotype }"
							width="200px" />

					</row>
					<row>
						<hbox align="center">
							<label value="*" style="color:#ff0000" />
							<label value="生产企业经济类型(新国标代码):" />
						</hbox>
						<textbox
							value="@{frmfactureEdit$composer.manu.manuEcotypeNew }"
							width="200px" />

					</row>
					<row>
						<hbox align="center">
							<label value="*" style="color:#ff0000" />
							<label value="生产企业经营范围:" />
						</hbox>
						<textbox
							value="@{frmfactureEdit$composer.manu.manuEcotypeNew }"
							width="200px" />

					</row>
					<row>
						<hbox align="center">
							<label value="*" style="color:#ff0000" />
							<label value="生产企业标志:" />
