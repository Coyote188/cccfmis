<?page title="new page title" contentType="text/html;charset=UTF-8"?>
<?init class="org.zkoss.zkplus.databind.AnnotateDataBinderInit" root="./frmfactureEdit"?>
<zk>
	<window id="frmfactureEdit"
		apply="cccf.ma.web.zk.ManuEditController">
		<style>.grd-inner {background: white; border: 0; }</style>
		<groupbox width="99%">
			<grid zclass="grd">
				<columns>
					<column width="30%" />
					<column width="70%" />
				</columns>
				<rows>
					<row>
						<hbox align="center">
							<label value="*" style="color:#ff0000" />
							<label value="生产企业名称:" />
						</hbox>
						<textbox
							value="@{frmfactureEdit$composer.manu.name }" width="200px" />

					</row>
					<row>
						<hbox align="center">
							<label value="*" style="color:#ff0000" />
							<label value="生产企业名称(英文):" />
						</hbox>
						<textbox
							value="@{frmfactureEdit$composer.manu.manuNameEn }"
							width="200px" />

					</row>
					<row>
						<hbox align="center">
							<label value="*" style="color:#ff0000" />
							<label value="生产企业组织机构代码:" />
						</hbox>
						<textbox
							value="@{frmfactureEdit$composer.manu.manucode }" width="200px" />

					</row>
					<row>
						<hbox align="center">
							<label value="*" style="color:#ff0000" />
							<label value="生产企业法人(负责人):" />
						</hbox>
